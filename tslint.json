{
  "extends": "tslint:recommended",
  "rulesDirectory": ["./demo/"],
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": false,
    "only-arrow-functions": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}